{
  "name": "@openapi-integration/request",
  "version": "0.0.1",
  "description": "a bunch of hooks methods for React to integration API",
  "main": "index.js",
  "scripts": {
    "test": "jest --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/teobler/request.git"
  },
  "keywords": [
    "hooks",
    "request",
    "api-integration"
  ],
  "author": "teobler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teobler/request/issues"
  },
  "homepage": "https://github.com/teobler/request#readme",
  "peerDependencies": {
    "lodash": ">=4.17.x",
    "react": ">=16.9.x",
    "redux": ">=4.0.x",
    "rxjs": ">=6.5.x"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.1.4",
    "@types/lodash": "4.14.138",
    "@types/node": "^12.12.31",
    "@types/prettier": "^1.19.1",
    "@types/react": "^16.9.25",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "axios-mock-adapter": "1.17.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^25.2.3",
    "prettier": "^2.0.2",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*/__tests__/.+\\.test\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/__tests__/**",
      "!src/actions.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
